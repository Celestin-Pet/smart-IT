{
    "expo": {
      "name": "JobApp",
      "slug": "jobapp",
      "version": "1.0.0",
      "orientation": "portrait",
      "icon": "./assets/google-logo.png",
      "splash": {
        "image": "./assets/bg-image.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff"
      },
      "updates": {
        "fallbackToCacheTimeout": 0
      },
      "assetBundlePatterns": [
        "**/*"
      ],
      "ios": {
        "supportsTablet": true
      },
      "android": {
        "adaptiveIcon": {
          "foregroundImage": "./assets/google-logo.png",
          "backgroundColor": "#ffffff"
        }
      },
      "web": {
        "favicon": "./assets/google-logo.png"
      }
    }
  }
  